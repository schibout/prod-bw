<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RV_QuantitePrestationNombreEntierPresta</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(PRESTA_TECH_Quantite__c,&quot;[0-9]{1}&quot;))</errorConditionFormula>
    <errorDisplayField>Quantity</errorDisplayField>
    <errorMessage>La quantité doit être un chiffre ou un nombre entier.</errorMessage>
</ValidationRule>
